{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
       "quotemark": [ false ],
       "align": false,
       "no-empty": [ false ],
       "no-empty-interface": [ false ],
       "no-unused-expression": false,
       "object-literal-sort-keys": false,
       "max-classes-per-file": [ true, 3, "exclude-class-expressions" ],
       "radix": false,
       "forin": false,
       "interface-name": false
    },
    "rulesDirectory": [],
    "typedef": [
       true,
       "call-signature",
       "property-declaration"
    ]
}
